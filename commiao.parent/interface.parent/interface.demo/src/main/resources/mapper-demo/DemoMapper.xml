<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.commiao.inteface.demo.mapper.DemoMapper">

	<resultMap id="demoEntity" type="com.commiao.inteface.demo.entities.DemoEntity">
		<result column="id" property="id" jdbcType="VARCHAR"/>
	</resultMap>

	<select id="testByPage" resultMap="demoEntity">
		select '1' as id from dual
		union all
		select '2' as id from dual
		union all
		select '3' as id from dual
		union all
		select '4' as id from dual
		union all
		select '5' as id from dual
	</select>

</mapper>

